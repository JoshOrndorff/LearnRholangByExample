new result, grandma in {
  // I send the message
  grandma!("Meet at the mall at 4:00")
  |
  // I double check the message
  for (msg <- grandma){
    result!("Double checking the message")
    |
    // Put a copy back on the channel for grandma
    grandma!(*msg)
  }
  |
  // Grandma receives it
  for (msg <- grandma){
    result!("I got your message")
  }
}
